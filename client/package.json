    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-config-shakacode": "^0.0.1",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "jade": "^1.11.0",
    "jscs": "^2.8.0",
    "node-sass": "^3.4.2",
    "react-transform-hmr": "^1.0.1",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
